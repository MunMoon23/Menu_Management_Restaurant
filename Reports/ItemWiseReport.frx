      <TableDataSource Name="Table" Alias="Revenue" DataType="System.Int32" Enabled="true" SelectCommand="SELECT &#13;&#10;    D.DishName,&#13;&#10;    BD.UnitPrice,&#13;&#10;    SUM(BD.Quantity) AS SoLuong,&#13;&#10;    SUM(BD.Quantity * BD.UnitPrice) AS ThanhTien&#13;&#10;FROM BillDetails BD&#13;&#10;JOIN Dishes D ON BD.DishID = D.DishID&#13;&#10;JOIN Bills B ON BD.BillID = B.BillID&#13;&#10;WHERE B.OrderTime BETWEEN @FromDate AND @ToDate&#13;&#10;      AND B.Status = N'Done'&#13;&#10;GROUP BY D.DishName, BD.UnitPrice&#13;&#10;&#13;&#10;UNION ALL&#13;&#10;&#13;&#10;SELECT &#13;&#10;    N'TỔNG CỘNG' AS DishName,&#13;&#10;    NULL AS UnitPrice,&#13;&#10;    SUM(BD.Quantity),&#13;&#10;    SUM(BD.Quantity * BD.UnitPrice)&#13;&#10;FROM BillDetails BD&#13;&#10;JOIN Dishes D ON BD.DishID = D.DishID&#13;&#10;JOIN Bills B ON BD.BillID = B.BillID&#13;&#10;WHERE B.OrderTime BETWEEN @FromDate AND @ToDate&#13;&#10;      AND B.Status = N'Done';">
        <Column Name="DishName" DataType="System.String"/>
        <Column Name="UnitPrice" DataType="System.Decimal"/>
        <Column Name="SoLuong" DataType="System.Int32"/>
        <Column Name="ThanhTien" DataType="System.Decimal"/>
        <CommandParameter Name="FromDate" DataType="22"/>
        <CommandParameter Name="ToDate" DataType="22"/>
